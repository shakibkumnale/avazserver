{
    "version": 2,
    "builds": [
      {
        "src": "src/appp.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "/appp.js"
      },
      {
        "src": "/(.*)", 
        "dest": "/index.html"
      }
    ]
  }
  