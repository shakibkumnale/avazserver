{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      
      }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "/app.js"
      },
      {
        "src": "/html/(.*)", 
        "dest": "/index.html"
      }
    ]
  }
  